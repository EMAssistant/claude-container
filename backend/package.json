{
  "name": "claude-container-backend",
  "version": "1.0.0",
  "description": "Backend server for Claude Container with Express and WebSocket support",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "claude",
    "container",
    "backend",
    "express",
    "websocket"
  ],
  "author": "Kyle",
  "license": "ISC",
  "dependencies": {
    "chokidar": "^4.0.0",
    "express": "^4.18.0",
    "js-yaml": "^4.1.1",
    "node-pty": "^1.0.0",
    "simple-git": "^3.30.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
